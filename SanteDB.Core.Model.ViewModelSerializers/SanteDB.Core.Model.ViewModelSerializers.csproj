        </ProjectReference>
        <ProjectReference Include="..\..\santedb-model\SanteDB.Core.Model\SanteDB.Core.Model.csproj">
          <Project>{f1a82a4f-4a57-47d7-ae76-444c8a4ff54a}</Project>
          <Name>SanteDB.Core.Model</Name>
        </ProjectReference>
